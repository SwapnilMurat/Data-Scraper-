            print(f"  Ocella: {data['ocella']} ({data['ocella']/data['total']*100:.1f}%)")
            print(f"  Custom: {data['custom']} ({data['custom']/data['total']*100:.1f}%)")
            print()
    
    print("URL PATTERN ANALYSIS:")
    print("-" * 30)
    patterns = analyze_url_patterns()
    for system, pattern_data in patterns.items():
        print(f"{system.upper()} System:")
        for pattern, count in pattern_data.items():
            print(f"  {pattern}: {count}")
        print()
    
    print("EXPORT OPTIONS:")
    print("-" * 30)
    print("Run with --export-csv to export CSV")
    print("Run with --export-json to export JSON")
    print()
    
    import sys
    if "--export-csv" in sys.argv:
        export_to_csv()
    if "--export-json" in sys.argv:
        export_to_json()
    
    print("SAMPLE COUNCILS BY SYSTEM TYPE:")
    print("-" * 40)
    
    uniform_councils = get_councils_by_system("uniform")[:5]
    print(f"UNIFORM System (sample of {len(uniform_councils)} from {len(get_councils_by_system('uniform'))} total):")
    for council in uniform_councils:
        print(f"  {council['name']} ({council['country']}): {council['url']}")
    print()
    
    ocella_councils = get_councils_by_system("ocella")[:5]
    print(f"OCELLA System (sample of {len(ocella_councils)} from {len(get_councils_by_system('ocella'))} total):")
    for council in ocella_councils:
        print(f"  {council['name']} ({council['country']}): {council['url']}")
    print()
    
    custom_councils = get_councils_by_system("custom")[:5]
    print(f"CUSTOM System (sample of {len(custom_councils)} from {len(get_councils_by_system('custom'))} total):")
    for council in custom_councils:
        print(f"  {council['name']} ({council['country']}): {council['url']}")
    print()
    
    print("REGIONAL BREAKDOWN:")
    print("-" * 30)
    regions = {}
    for council in COMPLETE_UK_COUNCILS:
        region = council.get('region', 'Unknown')
        if region not in regions:
            regions[region] = 0
        regions[region] += 1
    
    for region, count in sorted(regions.items()):
        print(f"{region}: {count} councils")



def get_high_priority_councils():
    """Getting councils that should be prioritized for scraping (major cities, uniform systems)"""
    high_priority = []
    
    priority_names = [
        "Birmingham", "Manchester", "Leeds", "Liverpool", "Sheffield", "Bristol",
        "Cardiff", "Edinburgh", "Glasgow", "Belfast", "Newcastle", "Nottingham",
        "Bradford", "Leicester", "Coventry", "Hull", "Stoke-on-Trent", "Derby",
        "Southampton", "Portsmouth", "Plymouth", "Wolverhampton", "Bolton"
    ]
    
    for council in COMPLETE_UK_COUNCILS:
        if (council["name"] in priority_names or 
            council["system_type"] == "uniform" or
            "london" in council.get("region", "").lower()):
            high_priority.append(council)
    
    return high_priority

def get_testing_subset():
    """Getting a small subset of councils for testing purposes"""
    testing_councils = []
    
    uniform_test = [c for c in COMPLETE_UK_COUNCILS if c["system_type"] == "uniform"][:3]
    ocella_test = [c for c in COMPLETE_UK_COUNCILS if c["system_type"] == "ocella"][:2]  
    custom_test = [c for c in COMPLETE_UK_COUNCILS if c["system_type"] == "custom"][:2]
    
    testing_councils.extend(uniform_test)
    testing_councils.extend(ocella_test)
    testing_councils.extend(custom_test)
    
    return testing_councils

def generate_scraping_schedule():
    """Generating an optimal scraping schedule based on system types and priorities"""
    schedule = {
        "batch_1_uniform_major": [],
        "batch_2_uniform_remaining": [],
        "batch_3_ocella": [],
        "batch_4_custom_high_priority": [],
        "batch_5_custom_remaining": []
    }
    
    high_priority = get_high_priority_councils()
    
    for council in COMPLETE_UK_COUNCILS:
        if council["system_type"] == "uniform":
            if council in high_priority:
                schedule["batch_1_uniform_major"].append(council)
            else:
                schedule["batch_2_uniform_remaining"].append(council)
        elif council["system_type"] == "ocella":
            schedule["batch_3_ocella"].append(council)
        else:  # custom
            if council in high_priority:
                schedule["batch_4_custom_high_priority"].append(council)
            else:
                schedule["batch_5_custom_remaining"].append(council)
    
    return schedule

def validate_urls():
    """Basic URL validation for the dataset"""
    issues = []
    
    for council in COMPLETE_UK_COUNCILS:
        url = council["url"]
        
        if not url.startswith(("http://", "https://")):
            issues.append(f"{council['name']}: URL missing protocol")
        
        if " " in url:
            issues.append(f"{council['name']}: URL contains spaces")
        
        if "example.com" in url.lower():
            issues.append(f"{council['name']}: Placeholder URL detected")
        
        if council["system_type"] == "uniform" and "online-applications" not in url and "publicaccess" not in url:
            issues.append(f"{council['name']}: Uniform system but URL doesn't match pattern")
        
        if council["system_type"] == "ocella" and "ocellaweb" not in url.lower():
            issues.append(f"{council['name']}: Ocella system but URL doesn't match pattern")
    
    return issues

def create_council_config_files():
    """Creating individual configuration files for each council"""
    import os
    import yaml
    
    config_dir = "council_configs"
    os.makedirs(config_dir, exist_ok=True)
    
    for council in COMPLETE_UK_COUNCILS:
        config_filename = f"{council['name'].lower().replace(' ', '_').replace('-', '_')}.yaml"
        config_path = os.path.join(config_dir, config_filename)
        
        config_data = {
            "name": council["name"],
            "country": council["country"],
            "region": council.get("region", "Unknown"),
            "url": council["url"],
            "system_type": council["system_type"],
            "rate_limit": 1.0 if council["system_type"] == "uniform" else 2.0,
            "max_retries": 3,
            "timeout": 30,
            "headers": {
                "User-Agent": "UK Planning Data Research Bot 1.0"
            }
        }
        
        if council["system_type"] == "uniform":
            config_data["selectors"] = {
                "application_rows": ["tr.searchresult", "div.application-row"],
                "application_id": ["a.application-link", "span.ref-number"],
                "address": ["td.address", "div.location"],
                "description": ["td.description", "div.proposal"],
                "status": ["td.status", "span.decision"]
            }
        elif council["system_type"] == "ocella":
            config_data["selectors"] = {
                "application_table": "table.results",
                "application_rows": "tr.odd, tr.even",
                "pagination": "a.next"
            }
        else:  
            config_data["selectors"] = {
                "application_containers": ["div.planning-application", "div.search-result"],
                "fallback_search": ["div[class*='application']", "tr[class*='result']"]
            }
        
        with open(config_path, 'w') as f:
            yaml.dump(config_data, f, default_flow_style=False, indent=2)
    
    print(f"Created {len(COMPLETE_UK_COUNCILS)} configuration files in {config_dir}/")


def get_reliable_councils():
    """Get councils known to have reliable, stable systems"""
    reliable = []
    
    for council in COMPLETE_UK_COUNCILS:
        if council["system_type"] == "uniform":
            reliable.append(council)
        elif council["name"] in ["Birmingham", "Manchester", "Cardiff", "Edinburgh"]:
            reliable.append(council)
    
    return reliable

def estimate_scraping_time():
    """Estimating total time needed to scrape all councils"""
    estimates = {
        "uniform": 30,  # seconds per council (including rate limiting)
        "ocella": 45,   # seconds per council  
        "custom": 60    # seconds per council (more complex parsing)
    }
    
    total_time = 0
    for council in COMPLETE_UK_COUNCILS:
        total_time += estimates[council["system_type"]]
    
    hours = total_time / 3600
    return {
        "total_seconds": total_time,
        "total_hours": round(hours, 2),
        "estimated_duration": f"{int(hours//1)}h {int((hours%1)*60)}m"
    }


def check_data_completeness():
    """Checking for missing or incomplete data in the dataset"""
    issues = []
    required_fields = ["name", "country", "url", "system_type"]
    
    for i, council in enumerate(COMPLETE_UK_COUNCILS):
        for field in required_fields:
            if field not in council or not council[field]:
                issues.append(f"Council {i}: Missing {field}")
        
        if len(council.get("url", "")) < 20:
            issues.append(f"{council.get('name', f'Council {i}')}: URL too short")
    
    return issues
