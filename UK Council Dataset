def get_system_statistics():
    """Getting statistics about planning system types across UK councils"""
    stats = {
        "uniform": 0,
        "ocella": 0,  
        "custom": 0,
        "total": len(COMPLETE_UK_COUNCILS)
    }
    
    by_country = {
        "England": {"uniform": 0, "ocella": 0, "custom": 0, "total": 0},
        "Wales": {"uniform": 0, "ocella": 0, "custom": 0, "total": 0},
        "Scotland": {"uniform": 0, "ocella": 0, "custom": 0, "total": 0},
        "Northern Ireland": {"uniform": 0, "ocella": 0, "custom": 0, "total": 0}
    }
    
    for council in COMPLETE_UK_COUNCILS:
        system = council["system_type"]
        country = council["country"]
        
        stats[system] += 1
        by_country[country][system] += 1
        by_country[country]["total"] += 1
    
    return stats, by_country

def export_to_csv(filename="uk_councils_complete.csv"):
    """Exporting council data to CSV"""
    import csv
    
    with open(filename, 'w', newline='', encoding='utf-8') as csvfile:
        fieldnames = ['name', 'country', 'url', 'system_type', 'region']
        writer = csv.DictWriter(csvfile, fieldnames=fieldnames)
        
        writer.writeheader()
        for council in COMPLETE_UK_COUNCILS:
            writer.writerow(council)
    
    print(f"Exported {len(COMPLETE_UK_COUNCILS)} councils to {filename}")

def export_to_json(filename="uk_councils_complete.json"):
    """Exporting council data to JSON"""
    import json
    
    data = {
        "metadata": {
            "total_councils": len(COMPLETE_UK_COUNCILS),
            "export_date": "2024-12-28",
            "countries": ["England", "Wales", "Scotland", "Northern Ireland"]
        },
        "councils": COMPLETE_UK_COUNCILS
    }
    
    with open(filename, 'w', encoding='utf-8') as jsonfile:
        json.dump(data, jsonfile, indent=2, ensure_ascii=False)
    
    print(f"Exported {len(COMPLETE_UK_COUNCILS)} councils to {filename}")

def get_councils_by_system(system_type):
    """Getting councils using a specific planning system"""
    return [c for c in COMPLETE_UK_COUNCILS if c["system_type"] == system_type]

def get_councils_by_country(country):
    """Getting councils from a specific country"""
    return [c for c in COMPLETE_UK_COUNCILS if c["country"] == country]

def get_councils_by_region(region):
    """Getting councils from a specific region"""
    return [c for c in COMPLETE_UK_COUNCILS if c.get("region") == region]

def analyze_url_patterns():
    """Analyzing URL patterns across different system types"""
    patterns = {
        "uniform": {
            "online-applications": 0,
            "publicaccess": 0,
            "planning": 0,
            "other": 0
        },
        "ocella": {
            "OcellaWeb": 0,
            "planningSearch": 0,
            "other": 0
        },
        "custom": {
            "gov.uk": 0,
            "planning": 0,
            "other": 0
        }
    }
    
    for council in COMPLETE_UK_COUNCILS:
        url = council["url"].lower()
        system = council["system_type"]
        
        if system == "uniform":
            if "online-applications" in url:
                patterns["uniform"]["online-applications"] += 1
            elif "publicaccess" in url:
                patterns["uniform"]["publicaccess"] += 1
            elif "planning" in url:
                patterns["uniform"]["planning"] += 1
            else:
                patterns["uniform"]["other"] += 1
        elif system == "ocella":
            if "ocellaweb" in url:
                patterns["ocella"]["OcellaWeb"] += 1
            elif "planningsearch" in url:
                patterns["ocella"]["planningSearch"] += 1
            else:
                patterns["ocella"]["other"] += 1
        else:  # custom
            if "gov.uk" in url:
                patterns["custom"]["gov.uk"] += 1
            elif "planning" in url:
                patterns["custom"]["planning"] += 1
            else:
                patterns["custom"]["other"] += 1
    
    return patterns

if __name__ == "__main__":
    stats, by_country = get_system_statistics()
    
    print("UK COUNCIL PLANNING SYSTEMS ANALYSIS")
    print("=" * 50)
    print(f"Total Councils: {stats['total']}")
    print(f"Uniform System: {stats['uniform']} ({stats['uniform']/stats['total']*100:.1f}%)")
    print(f"Ocella System: {stats['ocella']} ({stats['ocella']/stats['total']*100:.1f}%)")
    print(f"Custom Systems: {stats['custom']} ({stats['custom']/stats['total']*100:.1f}%)")
    print()
    
    print("BY COUNTRY:")
    print("-" * 30)
    for country, data in by_country.items():
